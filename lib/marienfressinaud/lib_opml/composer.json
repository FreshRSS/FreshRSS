{
    "name": "marienfressinaud/lib_opml",
    "description": "A library to read and write OPML in PHP.",
    "license": "MIT",
    "authors": [
        {
            "name": "Marien Fressinaud",
            "email": "dev@marienfressinaud.fr"
        }
